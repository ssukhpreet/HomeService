
@{
    ViewBag.Title = "Login";
}

<div class="container">
        <div class="row">
                <div class="col-md-12">
                    <form method="post" action="adminLogin">
                        <div class="form-group-lg">
                            <label>Enter User Name </label>  <br />
                            <input type="text" name="txtUserName" placeholder="Enter Name " class="form-control" />
                        </div>
                        <div class="form-group-lg">
                            <label>Enter User Password </label>  <br />
                            <input type="password" name="txtUserPassword" placeholder="Enter Password " class="form-control" />
                        </div>

                        <input type="submit"  class="btn btn-primary" value="sign In "/>


                    </form>
                </div>


        </div>


</div>

